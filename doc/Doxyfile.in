PROJECT_NAME = "@PROJECT_NAME@"
PROJECT_NUMBER = "@PROJECT_VERSION@"
OUTPUT_DIRECTORY = "@OUTPUT_DIR@"
INPUT += "@SOURCE_DIR@/include/"
INPUT += "@SOURCE_DIR@/src/"
RECURSIVE = YES
TAB_SIZE = 4
OPTIMIZE_OUTPUT_FOR_C = YES
EXTRACT_ALL = YES
SOURCE_BROWSER = YES
STRIP_CODE_COMMENTS = YES
SEARCHENGINE = YES
GENERATE_TREEVIEW = YES
GENERATE_LATEX = NO
TYPEDEF_HIDES_STRUCT = YES
INLINE_SOURCES = YES
DISABLE_INDEX = NO
FULL_SIDEBAR = NO
HTML_COLORSTYLE = LIGHT
HTML_EXTRA_STYLESHEET += "@SOURCE_DIR@/doc/theme/doxygen-awesome.css"
HTML_EXTRA_STYLESHEET += "@SOURCE_DIR@/doc/theme/doxygen-awesome-sidebar-only.css"
